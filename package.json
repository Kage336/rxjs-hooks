{
  "name": "rxjs-hooks-taro",
  "version": "0.0.1",
  "description": "Taro hooks for RxJS",
  "main": "dist/index.js",
  "author": "LongYinan <lynweklm@gmail.com>",
  "license": "MIT",
  "typings": "dist/cjs/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "lint": "tslint -c tslint.json -p src/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:LeetCode-OpenSource/rxjs-hooks.git"
  },
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "husky": "^2.0.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "rxjs": "^6.5.2",
    "sinon": "^7.2.4",
    "source-map-loader": "^0.2.4",
    "standard": "^12.0.1",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@tarojs/taro": "^1.3.0-beta.5",
    "tslib": "^1.9.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ],
    "*.@(ts|tsx)": [
      "prettier --write",
      "tslint -c tslint.json --fix",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always",
    "parser": "typescript"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
